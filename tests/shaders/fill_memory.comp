// glslang -V shader.comp
#version 460

layout(binding = 0) buffer Input {
   uint values[ ];
};

layout (local_size_x = 1, local_size_y = 1, local_size_z = 1) in;

const uint BUFFER_ELEMENTS = 32;

void main()
{
	uint index = gl_GlobalInvocationID.x;
	if (index >= BUFFER_ELEMENTS)
		return;
	values[index] = index;
}